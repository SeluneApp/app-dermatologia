{
  "expo": {
    "name": "Selune",
    "slug": "Selune",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Logo_Lua.png",
    "scheme": "selune",
    "userInterfaceStyle": "automatic",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.Selune"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Logo_Lua.png",
          "resizeMode": "contain",
          "backgroundColor": "#191970"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "19cd0320-d1f7-4b1c-9ecd-82f330efbd65"
      }
    }
  }
}
